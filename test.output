文法

    0 $accept: program $end

    1 program: function

    2 function: function block
    3         | %empty

    4 block: type ID ';'
    5      | type ID '[' NUM ']' ';'
    6      | type ID '(' param_list ')' '{' stmt_list '}'

    7 stmt: ';'
    8     | expr ';'
    9     | PRINT expr ';'
   10     | READ expr ';'
   11     | ID '=' expr ';'
   12     | ID '[' NUM ']' '=' expr ';'
   13     | BREAK ';'
   14     | RETURN expr ';'
   15     | WHILE '(' expr ')' stmt
   16     | IF '(' expr ')' stmt ELSE stmt
   17     | '{' stmt_list '}'

   18 stmt_list: stmt
   19          | stmt_list stmt

   20 type: INT
   21     | CHAR

   22 expr: NUM
   23     | ID
   24     | ID '[' NUM ']'
   25     | type ID
   26     | type ID '[' NUM ']'
   27     | '-' expr
   28     | '!' expr
   29     | expr '+' expr
   30     | expr '-' expr
   31     | expr '*' expr
   32     | expr '/' expr
   33     | expr '<' expr
   34     | expr '>' expr
   35     | expr GE expr
   36     | expr LE expr
   37     | expr NE expr
   38     | expr EQ expr
   39     | expr AND expr
   40     | expr OR expr
   41     | '(' expr ')'
   42     | ID '(' expr_list ')'

   43 expr_list: expr
   44          | expr_list ',' expr

   45 param_list: type ID
   46           | type ID '[' ']'
   47           | type ID ',' param_list
   48           | type ID '[' ']' ',' param_list
   49           | %empty


